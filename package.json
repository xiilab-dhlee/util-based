{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env MSW_ENABLE=false next dev",
    "dev:mock": "cross-env MSW_ENABLE=true next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "organize-imports": "biome check --write",
    "test": "cross-env MSW_ENABLE=true playwright test",
    "test:integration": "pnpm check-bddgen && cross-env MSW_ENABLE=false playwright test",
    "test:mock": "pnpm check-bddgen && cross-env MSW_ENABLE=true playwright test",
    "test:ui": "pnpm check-bddgen && cross-env MSW_ENABLE=false playwright test --ui",
    "test:headed": "pnpm check-bddgen && cross-env MSW_ENABLE=false playwright test --headed",
    "test:debug": "pnpm check-bddgen && cross-env MSW_ENABLE=false playwright test --debug",
    "test:chromium": "pnpm check-bddgen && cross-env MSW_ENABLE=false playwright test --project=chromium",
    "test:firefox": "pnpm check-bddgen && cross-env MSW_ENABLE=false playwright test --project=firefox",
    "test:webkit": "pnpm check-bddgen && cross-env MSW_ENABLE=false playwright test --project=webkit",
    "test:workload": "pnpm check-bddgen && cross-env MSW_ENABLE=false playwright test tests/features/workload",
    "test:smoke": "pnpm check-bddgen && cross-env MSW_ENABLE=false playwright test --grep @smoke",
    "test:report": "playwright show-report tests/reports/playwright-html",
    "test:trace": "playwright show-trace",
    "test:codegen": "cross-env MSW_ENABLE=false playwright codegen http://localhost:3000",
    "bddgen": "npx playwright-bdd export",
    "check-bddgen": "node scripts/check-bddgen-needed.js"
  },
  "dependencies": {
    "@anatine/zod-mock": "^3.14.0",
    "@ant-design/nextjs-registry": "^1.1.0",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@faker-js/faker": "^9.0.0",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@uiw/react-codemirror": "^4.25.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "ansi-colors": "^4.1.3",
    "antd": "^5.12.2",
    "apexcharts": "^5.3.0",
    "axios": "^1.13.1",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "es-toolkit": "^1.42.0",
    "jotai": "^2.12.5",
    "lodash": "^4.17.21",
    "next": "16.0.1",
    "next-auth": "^4.24.11",
    "react": "19.2.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "19.2.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^11.0.5",
    "remark-gfm": "^4.0.0",
    "split.js": "^1.6.5",
    "styled-components": "^6.1.19",
    "uuid": "^11.1.0",
    "xiilab-ui": "github:xiilab/xiilab-ui",
    "xterm-theme": "^1.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@playwright/test": "^1.56.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "1.0.0",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "msw": "^2.11.6",
    "playwright-bdd": "^8.4.1",
    "typescript": "^5"
  }
}
